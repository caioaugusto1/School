@model School_Project.Models.StudentsCourses.LinkCourseManyStudentsVM

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TeacherName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model.Students)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SurName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DOB)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Gender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address1)
                </td>
                <td>
                    <button type="button" class="btn btn-success">Included</button>
                    <button type="button" class="btn btn-danger" onclick="linkCourseManyStudents.includedStudentOneCourse(@Model.IdCourse, @item.Id)">Remove</button>
                </td>
            </tr>
        }
    </table>

    <script type="text/javascript" src="~/Scripts/Utils/Utils.js"></script>
    <script type="text/javascript" src="~/Scripts/CoursesStudents/LinkCourseManyStudents.js"></script>